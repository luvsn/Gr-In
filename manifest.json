{
    "name": "Gr-In",
    "version": "0.0.1",
    "description": "Ecological footprint for online shopping",
    "manifest_version": 3,
    "author": "Nima GOZALPOUR, Adrien GEETS, Lucas VISINTIN, Hugo KOLANDER",
    "action":{
        "default_popup": "index.html",
        "default_title": "Gr-In"
    },
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://amazon.de/*",
            "https://amazon.fr/*",
            "https://amazon.com/*"
          ]
        }
    ]
}